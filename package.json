{
  "name": "git-project-manager",
  "displayName": "gitProjectManager",
  "description": "Let you load your git projects in a easy way",
  "version": "0.0.1",
  "publisher": "FelipeCaputo",
  "engines": {
    "vscode": "^0.10.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:gitProjectManager.openProject",
    "onCommand:gitProjectManager.refreshProjects"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [{
        "command": "gitProjectManager.openProject",
        "title": "GPM: Open Git Project"
      },
      {
        "command": "gitProjectManager.refreshProjects",
        "title": "GPM: Refresh Projects"          
      }]
  },
  "devDependencies": {
    "vscode": "0.10.x"
  },
  "dependencies": {
    "filewalker": "^0.1.2"
  }
}
